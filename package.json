{
	"name": "latex",
	"displayName": "[DEPRECATED] LaTeX Language Support",
	"version": "0.0.1",
	"private": false,
	"description": "[DEPRECATED] LaTeX - colorizer, grammar and snippets.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension",
	"scripts": {
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "tsc -p ./"
	},
	"contributes": {
		"configuration": {
			"properties": {
				"latex.StopAsking": {
					"default": false,
					"description": "LaTeX: Don't ask again for deprication warning",
					"type": "boolean"
				}
			},
			"title": "LaTeX options"
		},
		"grammars": [
			{
				"language": "latex",
				"path": "./syntaxes/latex.tmLanguage",
				"scopeName": "text.tex.latex"
			},
			{
				"language": "tex",
				"path": "./syntaxes/tex.tmLanguage",
				"scopeName": "text.tex"
			}
		],
		"languages": [
			{
				"aliases": [
					"LaTeX",
					"latex"
				],
				"configuration": "./latex.configuration.json",
				"extensions": [
					".tex"
				],
				"id": "latex"
			},
			{
				"aliases": [
					"TeX",
					"tex"
				],
				"configuration": "./latex.configuration.json",
				"extensions": [
					".sty",
					".cls",
					".bbx",
					".cbx"
				],
				"id": "tex"
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"dependencies": {
		"open": "10.1.0"
	},
	"devDependencies": {
		"@types/mocha": "10.0.6",
		"@types/node": "20.11.30",
		"mocha": "10.3.0",
		"typescript": "5.4.2",
		"typescript-esbuild": "latest",
		"vscode": "1.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "images/iconLaTeX.png"
}
