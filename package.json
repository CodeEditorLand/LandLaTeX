{
	"name": "latex",
	"version": "1.0.0",
	"displayName": "[DEPRECATED] LaTeX Language Support",
	"description": "[DEPRECATED] LaTeX - colorizer, grammar and snippets.",
	"publisher": "ms-vscode",
	"icon": "images/iconLaTeX.png",
	"license": "SEE LICENSE IN LICENSE.txt",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-LaTeX.git"
	},
	"categories": [
		"Languages",
		"Snippets"
	],
	"engines": {
		"vscode": "1.6.0"
	},
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"title": "LaTeX options",
			"properties": {
				"latex.StopAsking": {
					"type": "boolean",
					"default": false,
					"description": "LaTeX: Don't ask again for deprication warning"
				}
			}
		},
		"languages": [
			{
				"id": "latex",
				"aliases": [
					"LaTeX",
					"latex"
				],
				"extensions": [
					".tex"
				],
				"configuration": "./latex.configuration.json"
			},
			{
				"id": "tex",
				"aliases": [
					"TeX",
					"tex"
				],
				"extensions": [
					".sty",
					".cls",
					".bbx",
					".cbx"
				],
				"configuration": "./latex.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "latex",
				"scopeName": "text.tex.latex",
				"path": "./syntaxes/latex.tmLanguage"
			},
			{
				"language": "tex",
				"scopeName": "text.tex",
				"path": "./syntaxes/tex.tmLanguage"
			}
		]
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"open": "10.1.0"
	},
	"devDependencies": {
		"typescript": "5.4.2",
		"vscode": "1.0.0",
		"mocha": "10.3.0",
		"@types/node": "20.11.28",
		"@types/mocha": "10.0.6"
	}
}
