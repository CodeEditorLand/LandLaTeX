{
  "name": "latex",
  "displayName": "[DEPRECATED] LaTeX Language Support",
  "version": "0.0.1",
  "private": false,
  "description": "[DEPRECATED] LaTeX - colorizer, grammar and snippets.",
  "homepage": "https://github.com/CodeEditorLand/Environment#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/Environment/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/Environment.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Nikola R. Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "publisher": "playform",
  "type": "module",
  "main": "./out/extension",
  "scripts": {
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
    "vscode:prepublish": "tsc -p ./"
  },
  "contributes": {
    "configuration": {
      "properties": {
        "latex.StopAsking": {
          "default": false,
          "description": "LaTeX: Don't ask again for deprication warning",
          "type": "boolean"
        }
      },
      "title": "LaTeX options"
    },
    "grammars": [
      {
        "language": "latex",
        "path": "./syntaxes/latex.tmLanguage",
        "scopeName": "text.tex.latex"
      },
      {
        "language": "tex",
        "path": "./syntaxes/tex.tmLanguage",
        "scopeName": "text.tex"
      }
    ],
    "languages": [
      {
        "aliases": [
          "LaTeX",
          "latex"
        ],
        "configuration": "./latex.configuration.json",
        "extensions": [
          ".tex"
        ],
        "id": "latex"
      },
      {
        "aliases": [
          "TeX",
          "tex"
        ],
        "configuration": "./latex.configuration.json",
        "extensions": [
          ".sty",
          ".cls",
          ".bbx",
          ".cbx"
        ],
        "id": "tex"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "dependencies": {
    "open": "0.0.5"
  },
  "devDependencies": {
    "@types/mocha": "2.2.32",
    "@types/node": "6.0.40",
    "mocha": "2.3.3",
    "typescript": "2.0.3",
    "typescript-esbuild": "0.3.6",
    "vscode": "1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "icon": "images/iconLaTeX.png"
}
