{
	"name": "latex",
	"displayName": "[DEPRECATED] LaTeX Language Support",
	"version": "0.0.1",
	"private": false,
	"description": "[DEPRECATED] LaTeX - colorizer, grammar and snippets.",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandLaTeX#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandLaTeX/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandLaTeX.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üõ¨‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -watch -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"configuration": {
			"properties": {
				"latex.StopAsking": {
					"default": false,
					"description": "LaTeX: Don't ask again for deprication warning",
					"type": "boolean"
				}
			},
			"title": "LaTeX options"
		},
		"grammars": [
			{
				"language": "latex",
				"path": "./syntaxes/latex.tmLanguage",
				"scopeName": "text.tex.latex"
			},
			{
				"language": "tex",
				"path": "./syntaxes/tex.tmLanguage",
				"scopeName": "text.tex"
			}
		],
		"languages": [
			{
				"aliases": [
					"LaTeX",
					"latex"
				],
				"configuration": "./latex.configuration.json",
				"extensions": [
					".tex"
				],
				"id": "latex"
			},
			{
				"aliases": [
					"TeX",
					"tex"
				],
				"configuration": "./latex.configuration.json",
				"extensions": [
					".sty",
					".cls",
					".bbx",
					".cbx"
				],
				"id": "tex"
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"dependencies": {
		"open": "10.1.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.12.10"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "images/iconLaTeX.png"
}
