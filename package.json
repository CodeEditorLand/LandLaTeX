{
  "name": "latex",
  "displayName": "[DEPRECATED] LaTeX Language Support",
  "version": "1.0.0",
  "description": "[DEPRECATED] LaTeX - colorizer, grammar and snippets.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-LaTeX.git"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "publisher": "ms-vscode",
  "main": "./out/extension",
  "scripts": {
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "tsc -p ./"
  },
  "contributes": {
    "configuration": {
      "properties": {
        "latex.StopAsking": {
          "default": false,
          "description": "LaTeX: Don't ask again for deprication warning",
          "type": "boolean"
        }
      },
      "title": "LaTeX options"
    },
    "grammars": [
      {
        "language": "latex",
        "path": "./syntaxes/latex.tmLanguage",
        "scopeName": "text.tex.latex"
      },
      {
        "language": "tex",
        "path": "./syntaxes/tex.tmLanguage",
        "scopeName": "text.tex"
      }
    ],
    "languages": [
      {
        "aliases": [
          "LaTeX",
          "latex"
        ],
        "configuration": "./latex.configuration.json",
        "extensions": [
          ".tex"
        ],
        "id": "latex"
      },
      {
        "aliases": [
          "TeX",
          "tex"
        ],
        "configuration": "./latex.configuration.json",
        "extensions": [
          ".sty",
          ".cls",
          ".bbx",
          ".cbx"
        ],
        "id": "tex"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "dependencies": {
    "open": "0.0.5"
  },
  "devDependencies": {
    "@types/mocha": "2.2.32",
    "@types/node": "6.0.40",
    "mocha": "2.3.3",
    "typescript": "2.0.3",
    "vscode": "1.0.0"
  },
  "icon": "images/iconLaTeX.png"
}
